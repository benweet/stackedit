{
  "name": "stackedit",
  "version": "5.13.3",
  "description": "Free, open-source, full-featured Markdown editor",
  "author": "Benoit Schweblin",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bbyy-me/stackedit/issues"
